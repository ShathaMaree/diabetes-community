{
    "name": "flarum/flarum",
    "description": "Delightfully simple forum software.",
    "type": "project",
    "keywords": [
        "forum",
        "discussion"
    ],
    "homepage": "https://flarum.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Flarum",
            "email": "info@flarum.org",
            "homepage": "https://flarum.org/team"
        }
    ],
    "support": {
        "issues": "https://github.com/flarum/core/issues",
        "source": "https://github.com/flarum/flarum",
        "docs": "https://docs.flarum.org/"
    },
    "require": {
        "askvortsov/flarum-moderator-warnings": "^0.6.3",
        "askvortsov/flarum-rich-text": "^2.1",
        "clarkwinkelmann/flarum-ext-anonymous-posting": "*",
        "clarkwinkelmann/flarum-ext-post-bookmarks": "^1.0",
        "flarum/approval": "*",
        "flarum/bbcode": "*",
        "flarum/core": "^1.8",
        "flarum/emoji": "*",
        "flarum/flags": "*",
        "flarum/lang-english": "*",
        "flarum/likes": "*",
        "flarum/lock": "*",
        "flarum/markdown": "*",
        "flarum/mentions": "*",
        "flarum/nicknames": "*",
        "flarum/pusher": "*",
        "flarum/statistics": "*",
        "flarum/sticky": "*",
        "flarum/subscriptions": "*",
        "flarum/suspend": "*",
        "flarum/tags": "^1.8",
        "fof/byobu": "*",
        "fof/custom-footer": "^1.1",
        "fof/pages": "^1.0",
        "fof/upload": "^1.8",
        "fof/user-directory": "^1.4",
        "local/diabetes-type": "@dev",
        "neoncube/flarum-private-messages": "*"
    },   
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "repositories": {
        "diabetes-type": {
            "type": "path",
            "url": "extensions/diabetes-type"
        }
    }
}
